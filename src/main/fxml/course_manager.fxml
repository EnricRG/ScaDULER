<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.CourseManagerController">
   <children>
      <TableView fx:id="courseTable" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="courseTable_nameColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Name" />
            <TableColumn fx:id="courseTable_descriptionColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="350.0" prefWidth="-1.0" text="Description" />
            <TableColumn fx:id="courseTable_firstQuarterResources" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Q1 Resources" />
            <TableColumn fx:id="courseTable_secondQuarterResources" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Q2 Resources" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" spacing="50.0">
         <children>
            <Button fx:id="addCourseButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Add Course">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="editCourseButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Edit Course">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="removeCourseButton" mnemonicParsing="false" prefWidth="200.0" text="Remove Course">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
