<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="eventTable" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="eventTable_nameColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Name" />
            <TableColumn fx:id="eventTable_shortNameColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Short Name" />
            <TableColumn fx:id="eventTable_subjectColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Subject" />
            <TableColumn fx:id="eventTable_resourceColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Resource" />
            <TableColumn fx:id="eventTable_weekColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="-1.0" text="Week" />
            <TableColumn fx:id="eventTable_durationColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="-1.0" text="Duration" />
            <TableColumn fx:id="eventTable_incompatibilitiesColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Incompatibilities" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" spacing="50.0">
         <children>
            <Button fx:id="addEventButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Add Event">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="editEventButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Edit Event">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="removeEventButton" mnemonicParsing="false" prefWidth="200.0" text="Remove Event">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
