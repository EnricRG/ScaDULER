<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.SubjectManagerController">
   <children>
      <TableView fx:id="subjectTable" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="subjectTable_nameColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="150.0" prefWidth="-1.0" text="Name" />
            <TableColumn fx:id="subjectTable_shortNameColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Short Name" />
            <TableColumn fx:id="subjectTable_descriptionColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="350.0" prefWidth="-1.0" text="Description" />
            <TableColumn fx:id="subjectTable_eventCountColumn" editable="false" maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="-1.0" text="Events" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" spacing="50.0">
         <children>
            <Button fx:id="addSubjectButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Add Subject">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="editSubjectButton" disable="true" mnemonicParsing="false" prefWidth="200.0" text="Edit Subject">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="removeSubjectButton" mnemonicParsing="false" prefWidth="200.0" text="Remove Subject">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
