<VBox alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane fx:id="borderPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="700.0" VBox.vgrow="ALWAYS">
         <center>
            <TabPane fx:id="tabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="510.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
               <tabs>
                  <Tab fx:id="subjectsTab" closable="false" text="Subjects" />
                  <Tab fx:id="coursesTab" closable="false" text="Courses" />
                  <Tab fx:id="eventsTab" closable="false" text="Events" />
                  <Tab fx:id="resourcesTab" closable="false" text="Resources" />
               </tabs>
            </TabPane>
         </center>
      </BorderPane>
      <Separator />
      <HBox alignment="TOP_CENTER" maxHeight="-Infinity" spacing="50.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel and undo changes">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="finishButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Finish and commit changes">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
