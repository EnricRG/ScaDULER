          <TableColumn fx:id="fieldNumberColumn" minWidth="75.0" prefWidth="-1.0" style="-fx-alignment: CENTER;" text="Field NÂº" />
            <TableColumn fx:id="fieldValueColumn" minWidth="200.0" prefWidth="-1.0" text="Field value" />
            <TableColumn fx:id="errorDescriptionColumn" minWidth="500.0" prefWidth="-1.0" text="Error Description" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
